{
	"extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
	"plugins": ["jsx-a11y", "react", "prettier"],
	"rules": {
		"react/jsx-max-props-per-line": [1, { "maximum": 1 }],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"no-useless-catch": 0
	}
}
