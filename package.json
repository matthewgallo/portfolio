{
 "name": "matthew-portfolio",
 "version": "0.1.0",
 "private": true,
 "proxy": "http://localhost:8081",
 "engines": {
  "node": "7.10.1",
  "npm": "4.1.2"
 },
 "devDependencies": {
  "babel-eslint": "^7.2.3",
  "babel-watch": "^2.0.7",
  "concurrently": "^3.5.0",
  "custom-react-scripts": "0.0.23",
  "husky": "^0.14.3",
  "lint-staged": "^4.0.2",
  "prettier": "^1.5.3"
 },
 "dependencies": {
  "babel-cli": "^6.24.1",
  "babel-preset-es2015": "^6.24.1",
  "babel-preset-react": "^6.24.1",
  "babel-preset-stage-2": "^6.24.1",
  "compression": "^1.7.0",
  "express": "^4.15.3",
  "path": "^0.12.7",
  "react": "^15.6.1",
  "react-dom": "^15.6.1",
  "react-modal": "^2.2.4",
  "react-router": "^4.2.0",
  "react-router-dom": "^4.2.2",
  "react-scripts": "1.0.10",
  "react-scroll": "^1.5.4",
  "request": "^2.79.0",
  "sw-precache": "^5.2.0"
 },
 "scripts": {
  "start": "concurrently \"PORT=8080 react-scripts start\" \"PORT=8081 babel-watch index.js\"",
  "precommit": "lint-staged",
  "build": "react-scripts build",
  "test": "react-scripts test --env=jsdom",
  "deploy": "now && now alias"
 },
 "lint-staged": {
  "src/**/*.js": [
   "prettier --trailing-comma es5 --single-quote --write",
   "git add"
  ],
  "index.js": [
   "prettier --trailing-comma es5 --single-quote --write",
   "git add"
  ]
 },
  "now": {
    "alias": [
      "matthewdgallo.com",
      "www.matthewdgallo.com"
    ]
  }
}
